 <tr>
  <td colspan="1" class="blank"> </td>
  <td colspan="2" class="total-line"><%= t('Subtotal ex. tax') %></td>
  <td class="total-value"><div class="subtotal"><%= formatMoney(rent.total.subTotal) %></div></td>
</tr>
<% if (rent.vats.length) { %>
<tr>
  <td colspan="1" class="blank"> </td>
  <td colspan="2" class="total-line"><%= t('VAT') %> <%= formatPercent(rent.vats[0].rate) %></td>
  <td class="total-value"><div class="subtotal"><%= formatMoney(rent.total.vat) %></div></td>
</tr>
<% } %>
<tr>
  <td colspan="1" class="blank"> </td>
  <td colspan="2" class="total-line"><%= t('Previous balance') %></td>
  <td class="total-value"><div class="subtotal"><%= formatMoney(rent.total.balance) %></div></td>
</tr>
<tr>
  <td colspan="1" class="blank"> </td>
  <td colspan="2" class="total-line"><%= t('Total') %></td>
  <td class="total-value"><div class="total"><%= formatMoney(rent.total.grandTotal) %></div></td>
</tr>
